# Enable board support
CONFIG_BOARD_VICHARAK_VAAMAN=y

# Enable RTL8822CS Wi-Fi support
CONFIG_RTL8822CS=m

# Disable unused UART BT drivers
# CONFIG_BT_HCIUART_3WIRE is not set
# CONFIG_BT_HCIUART_BCM is not set
# CONFIG_BT_HCIUART_H4 is not set
# CONFIG_BT_HCIUART_LL is not set
# CONFIG_BT_HCIUART_MRVL is not set
# CONFIG_BT_HCIUART_NOKIA is not set
# CONFIG_BT_HCIUART_QCA is not set
# CONFIG_BT_HCIUART_RTL is not set
# CONFIG_BT_MTKUART is not set
# CONFIG_BT_RTK_HCIUART_H4 is not set

# Enable Realtek UART bluetooth support
CONFIG_RTKBT_UART=y

# Enable serial device bus support
CONFIG_SERIAL_DEV_BUS=y
CONFIG_SERIAL_DEV_CTRL_TTYPORT=y

# Enable PCA953X GPIO expander support
CONFIG_GPIO_PCA953X=y

# Compile SPI rockchip as module
CONFIG_SPI_ROCKCHIP=m

# Enable overlay support
CONFIG_OF_OVERLAY=y

# Enable overlay fs support
CONFIG_OVERLAY_FS=y

# Enable Virtual camera support
CONFIG_VIDEO_VIRT_CAMERA=y

# Disable initrd support
# CONFIG_BLK_DEV_INITRD is not set

# Disable unused MALI drivers
# CONFIG_MALI400 is not set
# CONFIG_MALI450 is not set
# CONFIG_MALI_BIFROST is not set
