	vcc3v3_lcd_edp: vcc3v3-lcd-edp {
        compatible = "regulator-fixed";
        gpio = <&gpio0 RK_PC5 GPIO_ACTIVE_HIGH>;
        enable-active-high;
        regulator-name = "vcc3v3_lcd_edp";
        regulator-boot-on;
        regulator-state-mem {
        	regulator-off-in-suspend;
        };
	};

