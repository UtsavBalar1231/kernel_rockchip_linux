&hdmi_in_vopl {
	status = "disabled";
};

&hdmi_in_vopb {
	status = "okay";
};

&route_hdmi {
	status = "okay";
};

&route_dsi {
	status = "okay";
	connect = <&vopl_out_dsi>;
};

&dp_in_vopb {
	status = "disabled";
};

&dp_in_vopl {
	status = "disabled";
};

&edp_in_vopl {
	status = "disabled";
